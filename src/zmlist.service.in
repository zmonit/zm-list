[Unit]
Description=zmlist service
After=network.target
# Requires=network.target
# Conflicts=shutdown.target
# PartOf=zm-list.target

[Service]
Type=simple
# User=@uid@
Environment="prefix=@prefix@"
Environment='SYSTEMD_UNIT_FULLNAME=%n'
ExecStart=@prefix@/bin/zmlist @sysconfdir@/@PACKAGE@/zmlist.cfg
Restart=always

[Install]
WantedBy=multi-user.target
# WantedBy=zm-list.target
